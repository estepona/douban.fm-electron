{
  "files.autoSave": "afterDelay",
  "editor.formatOnSave": false,
  "eslint.autoFixOnSave": true,
  "editor.tabSize": 2,
  "eslint.validate": [
    "javascript",
    {
      "language": "typescript",
      "autoFix": true
    },
  ],
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.formatOnSave": true
  },
  "[css]": {
    "editor.formatOnSave": true
  },
  "cSpell.words": [
    "douban",
    "menu",
    "selected",
    "submenu"
  ]
}
